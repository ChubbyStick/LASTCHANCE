            <string>keyRight = keyboard_check(vk_right);

keyLeft = -keyboard_check(vk_left);

keyUp = keyboard_check(vk_up);

if keyboard_check_pressed(vk_space) &amp;&amp; global.goldCount &gt; 0
{
       audio_play_sound(Coin_drop_snd, 10, false);

       instance_create(PlayerCube_obj.x, PlayerCube_obj.y - 35, BrokenPiece_obj);
        
       global.goldCount -= 1;
       
       if global.playerweight &gt; 1
       {
            global.playerweight -= global.pieceweight;
       }
       
       
}


if keyboard_check(vk_right)
{
    image_xscale = 1;
}

if keyboard_check(vk_left)
{
    image_xscale = - 1; 
}



if (PlayerCube_obj.x &lt; 0)
{
    PlayerCube_obj.x = 0;
}

if (PlayerCube_obj.x &gt; 600)
{
    PlayerCube_obj.x = 600;
}

// Reaction

move = keyRight + keyLeft;
hsp = move * movespeed / global.playerweight;

//if (vsp &lt; 10 )
{
    vsp += grav * global.playerweight;
}

if (place_meeting(x , y + 1, StonePlatform_obj))
{
    vsp = keyUp * -jumpspeed ;    
}

//Horizontal collision
if (place_meeting(x + hsp, y, StonePlatform_obj))
{
    while(!place_meeting(x + sign(hsp), y, StonePlatform_obj))
    {
        x += sign(hsp);
    }
    
    hsp = 0;
}
x += hsp;

//Vertical collision
if (place_meeting(x, y + vsp, StonePlatform_obj))
{
    while(!place_meeting(x , y + sign(vsp) ,StonePlatform_obj))
    {
        y += sign(vsp);
    }
    vsp = 0;
}
y += vsp;

//diagonal collision
if(place_meeting(x+hsp, y+vsp, StonePlatform_obj))
{
    while(!place_meeting(x+sign(hsp), y + sign(vsp), StonePlatform_obj))
    {
        x+= sign(hsp);
        y+= sign(vsp);
    }
    hsp = 0;
    vsp = 0;
}

x += hsp;
y += vsp;





</string>
